{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f5f5f5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.neotavern.frontend"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.neotavern.frontend"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/Lexend_100Thin.ttf",
            "node_modules/@expo-google-fonts/Lexend_200ExtraLight.ttf",
            "node_modules/@expo-google-fonts/Lexend_300Light.ttf",
            "node_modules/@expo-google-fonts/Lexend_400Regular.ttf",
            "node_modules/@expo-google-fonts/Lexend_500Medium.ttf",
            "node_modules/@expo-google-fonts/Lexend_600SemiBold.ttf",
            "node_modules/@expo-google-fonts/Lexend_700Bold.ttf",
            "node_modules/@expo-google-fonts/Lexend_800ExtraBold.ttf",
            "node_modules/@expo-google-fonts/Lexend_900Black.ttf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "194b515a-64e0-45a8-aa94-53b94d8390d4"
      }
    },
    "owner": "neotavern",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/194b515a-64e0-45a8-aa94-53b94d8390d4"
    }
  }
}
